{{# if this.list }}
    <div class="row">
    <div class="col-sm-6 col-md-6 col-md-offset-3 col-sm-offset-3">

        <h1>Gekozen Jobs</h1>
        <br>
    <table class="table table-striped table-hover">
        <thead>
        <tr>
            <th>Naam</th>
            <th>Type</th>
            <th>â‚¬</th>
            <th>Plaats</th>
        </tr>
        </thead>
    <tbody>

    {{#each list}}

    <tr class="active">

        <td>{{ this.name }}</td>
        <td>{{ this.type }}</td>
        <td>{{ this.payabletopublish }}</td>
        <td>{{ this.placetopublish }}</td>
        <td>
            <a style=" position: absolute;left: 90%" href=""><i class="fa fa-comment-o" aria-hidden="true"></i></a>
            <a style=" position: absolute;left: 93%"href=""><i class="fa fa-trash-o" aria-hidden="true"></i></a>
        </td>
    </tr>
    {{/each}}
    </table>
    </div>
    </div>


{{ else }}
    <div class="row">
        <div class="col-lg-12 col-md-4 col-sm-6 col-lg-offset-0 col-md-offset-4 col-sm-offset-3">
            <h2>Nog geen jobs gekozen!</h2>
        </div>
    </div>
{{/if}}



